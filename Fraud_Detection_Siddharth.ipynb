   "outputs": [],
   "source": [
    "\n",
    "#User configuration\n",
    "CSV_PATH = \"\Fraud.csv\"      # change this\n",
    "SAVE_DIR = \".\\artifacts\"                   # where to save models & outputs\n",
    "INDEX_COL = None                           # set to a column name if the CSV has a unique index\n",
    "TIME_COL_CANDIDATES = [\"transaction_time\",\"TransactionDT\",\"event_time\",\"timestamp\",\"tx_datetime\"]\n",
    "TARGET_GUESSES = [\"isfraud\",\"fraud\",\"isFraud\",\"Class\",\"target\"]\n",
    "\n",
    "# For memory efficiency\n",
    "CHUNK_SIZE = 1_000_000     \n",
    "EDA_SAMPLE_ROWS = 500_000  \n",
    "\n",
    "RANDOM_STATE = 42\n"
   ]
