    "\n",
    "def evaluate_model(name, y_true, y_prob):\n",
    "    y_pred = (y_prob >= 0.5).astype(int)\n",
    "    roc = roc_auc_score(y_true, y_prob)\n",
    "    pr  = average_precision_score(y_true, y_prob)\n",
    "    f1  = f1_score(y_true, y_pred, zero_division=0)\n",
    "    cm  = confusion_matrix(y_true, y_pred)\n",
    "    print(f\"\\n=== {name} ===\")\n",
    "    print(\"ROC-AUC:\", round(roc, 4))\n",
    "    print(\"PR-AUC:\", round(pr, 4))\n",
    "    print(\"F1@0.50:\", round(f1, 4))\n",
    "    print(\"Confusion Matrix@0.50:\\n\", cm)\n",
    "    return {\"roc_auc\": roc, \"pr_auc\": pr, \"f1@0.50\": f1, \"cm\": cm.tolist()}\n",
    "\n",
    "# Validation evaluation\n",
    "logreg_valid_prob = log_reg.predict_proba(X_valid)[:,1]\n",
    "hgb_valid_prob   = hgb.predict_proba(X_valid_factor[hgb_features])[:,1]\n",
    "\n",
    "logreg_metrics = evaluate_model(\"LogisticRegression (valid)\", y_valid, logreg_valid_prob)\n",
    "hgb_metrics    = evaluate_model(\"HistGradientBoosting (valid)\", y_valid, hgb_valid_prob)\n",
    "\n",
    "# Pick the best by PR-AUC (suited for rare fraud)\n",
    "best_name = \"HGB\" if hgb_metrics[\"pr_auc\"] > logreg_metrics[\"pr_auc\"] else \"LogReg\"\n",
    "print(\"\\nSelected best model by PR-AUC:\", best_name)\n",
    "\n",
    "# Find an F1‑optimal threshold on validation\n",
    "def best_threshold(y_true, y_prob):\n",
    "    ps, rs, ts = precision_recall_curve(y_true, y_prob)\n",
    "    f1s = 2*ps*rs/(ps+rs+1e-12)\n",
    "    idx = np.nanargmax(f1s)\n",
    "    return ts[max(idx-1, 0)], ps[max(idx-1, 0)], rs[max(idx-1, 0)], f1s[idx]\n",
    "\n",
    "thr_lg, p_lg, r_lg, f1_lg = best_threshold(y_valid, logreg_valid_prob)\n",
    "thr_hg, p_hg, r_hg, f1_hg = best_threshold(y_valid, hgb_valid_prob)\n",
    "\n",
    "print(f\"Best threshold (LogReg): {thr_lg:.3f} | P={p_lg:.3f}, R={r_lg:.3f}, F1={f1_lg:.3f}\")\n",
    "print(f\"Best threshold (HGB):   {thr_hg:.3f} | P={p_hg:.3f}, R={r_hg:.3f}, F1={f1_hg:.3f}\")\n",
    "\n",
    "best_thr = thr_hg if best_name==\"HGB\" else thr_lg\n",
    "print(\"Chosen decision threshold:\", round(float(best_thr), 3))\n"
   ]
