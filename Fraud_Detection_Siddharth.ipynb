    "\n",
    "#Preprocess\n",
    "numeric_transformer = RobustScaler(with_centering=True, with_scaling=True, quantile_range=(25.0, 75.0))\n",
    "categorical_transformer = OneHotEncoder(handle_unknown=\"ignore\", sparse_output=True, min_frequency=0.01)\n",
    "\n",
    "preprocess = ColumnTransformer(\n",
    "    transformers=[\n",
    "        (\"num\", numeric_transformer, num_cols_final),\n",
    "        (\"cat\", categorical_transformer, cat_cols_final)\n",
    "    ],\n",
    "    remainder=\"drop\"\n",
    ")\n",
    "\n",
    "#Logistic Regression baseline \n",
    "log_reg = Pipeline(steps=[\n",
    "    (\"preprocess\", preprocess),\n",
    "    (\"clf\", LogisticRegression(\n",
    "        max_iter=200,\n",
    "        n_jobs=None,\n",
    "        class_weight=\"balanced\",\n",
    "        C=0.5,                \n",
    "        solver=\"saga\",\n",
    "        penalty=\"l2\",\n",
    "        verbose=0,\n",
    "        random_state=RANDOM_STATE\n",
    "    ))\n",
    "])\n",
    "\n",
    "#HistGradientBoosting (robust trees: handles missing numerics inherently)\n",
    "\n",
    "def factorize_frame(df, cats: List[str]):\n",
    "    out = df.copy()\n",
    "    mapping = {}\n",
    "    for c in cats:\n",
    "        codes, uniques = pd.factorize(out[c], sort=True)\n",
    "        out[c] = codes.astype(\"int32\")\n",
    "        mapping[c] = {str(u): int(i) for i,u in enumerate(uniques)}\n",
    "    return out, mapping\n",
    "\n",
    "X_train_factor, factor_maps = factorize_frame(X_train, cat_cols_final)\n",
    "X_valid_factor, _ = factorize_frame(X_valid, cat_cols_final)\n",
    "X_test_factor,  _ = factorize_frame(X_test,  cat_cols_final)\n",
    "\n",
    "hgb_features = num_cols_final + cat_cols_final\n",
    "\n",
    "hgb = HistGradientBoostingClassifier(\n",
    "    learning_rate=0.08,\n",
    "    max_depth=8,\n",
    "    max_leaf_nodes=31,\n",
    "    min_samples_leaf=200,      # regularize\n",
