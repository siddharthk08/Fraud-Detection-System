    "\n",
    "def guess_target_column(columns: List[str], guesses: List[str]) -> Optional[str]:\n",
    "    lower_map = {c.lower(): c for c in columns}\n",
    "    for g in guesses:\n",
    "        if g.lower() in lower_map:\n",
    "            return lower_map[g.lower()]\n",
    "    return None\n",
    "\n",
    "#read one small chunk to infer columns & a quick target guess\n",
    "first = pd.read_csv(CSV_PATH, nrows=50_000, index_col=INDEX_COL)\n",
    "all_cols = list(first.columns)\n",
    "TARGET_COL = guess_target_column(all_cols, TARGET_GUESSES)\n",
    "print(\"Columns:\", all_cols)\n",
    "print(\"Guessed TARGET_COL:\", TARGET_COL)\n",
    "\n",
    "# Detect likely numeric vs categorical (from the small sample)\n",
    "num_cols = first.select_dtypes(include=[np.number]).columns.tolist()\n",
    "cat_cols = [c for c in all_cols if c not in num_cols]\n",
    "print(\"Numeric cols (sample-based):\", num_cols)\n",
    "print(\"Categorical cols (sample-based):\", cat_cols)\n",
    "\n",
    "#iterate chunks to compute basic stats, class counts, and write an EDA sample\n",
    "eda_rows = []\n",
    "class_counts = {}\n",
    "total_rows = 0\n",
    "\n",
    "for chunk in pd.read_csv(CSV_PATH, chunksize=CHUNK_SIZE, index_col=INDEX_COL):\n",
    "    total_rows += len(chunk)\n",
    "\n",
    "    if TARGET_COL is None:\n",
    "        TARGET_COL = guess_target_column(chunk.columns, TARGET_GUESSES)\n",
    "\n",
    "    if TARGET_COL and TARGET_COL in chunk.columns:\n",
    "        vc = chunk[TARGET_COL].value_counts(dropna=False).to_dict()\n",
    "        for k,v in vc.items():\n",
    "            class_counts[k] = class_counts.get(k, 0) + v\n",
    "\n",
    "    # keep a stratified sample if target exists; else random\n",
    "    if TARGET_COL and TARGET_COL in chunk.columns:\n",
    "        # per-chunk stratified sample of small size\n",
    "        s = chunk.groupby(TARGET_COL, group_keys=False).apply(lambda df: df.sample(frac=min(1.0, EDA_SAMPLE_ROWS/10_000_000), random_state=RANDOM_STATE))\n",
    "        eda_rows.append(s)\n",
    "    else:\n",
    "        eda_rows.append(chunk.sample(frac=min(1.0, EDA_SAMPLE_ROWS/10_000_000), random_state=RANDOM_STATE))\n",
    "\n",
    "eda_df = pd.concat(eda_rows, axis=0, ignore_index=True)\n",
    "print(f\"Total rows scanned: {total_rows:,}\")\n",
    "if TARGET_COL:\n",
    "    print(\"Class counts (coarse):\", class_counts)\n",
    "\n",
    "# Save EDA sample for fast iteration\n",
    "eda_path = Path(SAVE_DIR) / \"eda_sample.parquet\"\n",
    "eda_df.to_parquet(eda_path, index=False)\n",
    "print(\"Saved EDA sample:\", eda_path)\n"
   ]
  },
